  <DataSources>
    <DataSource Name="budgetDB">
      <DataSourceReference>budgetDB</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c18766bb-354a-4864-b67f-c07463261e0e</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="costcategoryparent">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	a.Year
	,a.HKCategory
	,ISNULL(d.preYear, 0) AS preYear	
	,ISNULL(b.Budget, 0) AS Budget
	,ISNULL(e.curYear, 0) AS curYear
	,ISNULL(c.nextYear, 0) AS nexYear 
FROM 
	((select distinct year, HKCategory as HKCategory from CAPEX_Main where year = @Year) union (select distinct year, HKCategoryValue as HKCategory from CAPEX_Budget where year = @Year and ProjectBased not like 'CF from%')) as a
	LEFT JOIN 
	(SELECT Year,HKCategoryValue, SUM(CurrentY0) AS Budget FROM CAPEX_Budget WHERE (Year = @Year) GROUP BY Year,HKCategoryValue) AS b 
		ON a.HKCategory = b.HKCategoryValue
	LEFT JOIN 
	(SELECT Year, HKCategory, SUM(BABudgetDetails) AS curYear FROM CAPEX_Main WHERE (Year = @Year) GROUP BY Year,HKCategory) AS e 
		ON a.HKCategory = e.HKCategory 
	LEFT JOIN 
	(SELECT Year,HKCategory, SUM(BABudgetDetails) AS preYear FROM CAPEX_Main AS CAPEX_Main_1 WHERE (Year = @Year-1) GROUP BY Year,HKCategory) AS d 
		ON a.HKCategory = d.HKCategory 
	LEFT JOIN 
	(SELECT Year,HKCategoryValue, SUM(budgetdetail) AS nextYear FROM  CAPEX_Budget AS [Cost Category_1] WHERE  (Year = @Year+1) and ProjectBased not like 'CF from%' GROUP BY Year,HKCategoryValue) AS c 
		ON a.HKCategory = c.HKCategoryValue </CommandText>
      </Query>
