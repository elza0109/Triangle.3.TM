  <DataSets>
    <DataSet Name="ds_5yr">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <CommandText>select 
	ua.year,
	ua.Category,
	sum(ISNULL(cb.budget,0)) as budget, 
	sum(ISNULL(cm.consumed,0)) as consumed,
	ISNULL((select sum(ISNULL(CurrentY0,0)) from CAPEX_Budget where (CategoryValue + ' ' + NatureValue) = ua.Category and year = (YEAR(GETDATE()) + 1)),0)as Y1,
	ISNULL((select sum(ISNULL(CurrentY0,0)) from CAPEX_Budget where (CategoryValue + ' ' + NatureValue) = ua.Category and year = (YEAR(GETDATE()) + 2)),0) as Y2,
	ISNULL((select sum(ISNULL(CurrentY0,0)) from CAPEX_Budget where (CategoryValue + ' ' + NatureValue) = ua.Category and year = (YEAR(GETDATE()) + 3)),0) as Y3,
	ISNULL((select sum(ISNULL(CurrentY0,0)) from CAPEX_Budget where (CategoryValue + ' ' + NatureValue) = ua.Category and year = (YEAR(GETDATE()) + 4)),0) as Y4
from 
	((select distinct year, (Category+ ' ' + Nature) as Category from CAPEX_Main where year is not null) union (select distinct year, (CategoryValue+ ' ' + NatureValue) as Category from CAPEX_Budget where year is not null)) as ua left join
	(select year,(Category+ ' ' + Nature) as Category,sum(ISNULL(BABudgetDetails,0)) as consumed from CAPEX_Main group by year,Category,Nature) as cm on cm.Category = ua.Category and cm.Year = ua.Year left join 
	(select year,(CategoryValue+ ' ' + NatureValue) as Category,sum(ISNULL(CurrentY0,0)) as budget,sum(ISNULL(CurrentY0,0)) as Y1 from CAPEX_Budget group by year,CategoryValue,NatureValue) as cb on cb.Category = ua.Category and cb.Year = ua.Year 
group by ua.year,ua.Category</CommandText>
      </Query>
