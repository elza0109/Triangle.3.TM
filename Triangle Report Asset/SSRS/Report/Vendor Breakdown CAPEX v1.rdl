    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="vendorBreakDown">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>(select 
	CASE 
		WHEN HKVendorCategory in (select top 5 HKVendorCategory from CAPEX_Budget 
								where HKVendorCategory not in ('Staff cost','Other known vendor','Tender','HP','DELL','DELL EMC','Oracle','SUN/Oracle','Microsoft','Internal Charge from 3 solutions')
								group by HKVendorCategory 
								order by sum(BudgetDetail) desc) 
			THEN '1'
		WHEN HKVendorCategory = 'Tender' THEN '3' 
		WHEN HKVendorCategory = 'HP' THEN '5'  
		WHEN HKVendorCategory like 'DELL %' THEN '6'
		WHEN HKVendorCategory = 'Oracle' or HKVendorCategory = 'SUN/Oracle' THEN '7'
		WHEN HKVendorCategory = 'Microsoft' THEN '8'
		WHEN HKVendorCategory = 'Internal Charge from 3 solutions' THEN '9'
		ELSE '2'
	END AS post
	,Year
	,projectbased
	,HKCategoryValue as HKCategory
	,HKVendorCategory
	,sum(BudgetDetail) as val	
from CAPEX_Budget as a
where Year  = @year+1 and a.HKCategoryValue IS NOT NULL and HKVendorCategory != 'Staff cost' 
and projectbased not like case when Year  = 2019 then 'CF from%' ELSE '' end
group by Year,projectbased	,a.HKCategoryValue,HKVendorCategory
)union(
select 
	CASE 
		WHEN HKVendorCategory in (select top 5 HKVendorCategory from CAPEX_Main 
								where HKVendorCategory not in ('Staff cost','Other known vendor','Tender','HP','DELL','DELL EMC','Oracle','SUN/Oracle','Microsoft','Internal Charge from 3 solutions')
								group by HKVendorCategory 
								order by sum(BABudgetDetails) desc) 
			THEN '1'
		WHEN HKVendorCategory = 'Tender' THEN '3' 
		WHEN HKVendorCategory = 'HP' THEN '5'  
		WHEN HKVendorCategory like 'DELL %' THEN '6'
		WHEN HKVendorCategory = 'Oracle' or HKVendorCategory = 'SUN/Oracle' THEN '7'
		WHEN HKVendorCategory = 'Microsoft' THEN '8'
		WHEN HKVendorCategory = 'Internal Charge from 3 solutions' THEN '9'
		ELSE '2'
	END AS post
	,Year
	,projectbased
	,HKCategory	 
	,HKVendorCategory
	,sum(BABudgetDetails) as val	
from CAPEX_MAin as a
where Year  = @year and a.HKCategory IS NOT NULL and HKVendorCategory != 'Staff cost' 
and projectbased not like case when Year  = 2019 then 'CF from%' ELSE '' end
group by Year,projectbased	,a.HKCategory,HKVendorCategory)</CommandText>
      </Query>
      <Fields>
        <Field Name="post">
          <DataField>post</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="projectbased">
