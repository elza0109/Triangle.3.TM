  <DataSets>
    <DataSet Name="HKnProject">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <CommandText>select aa.Year,aa.Project_based,aa.Budget,isnull(bb.Vendor,'N/A') as Vendor,isnull(bb.Nature,'N/A') as Nature,isnull(bb.BA_Budget_Details,0) as BA_Budget_Details
from (select a.year, a.ProjectBased AS Project_based,ISNULL(SUM(a.CurrentY0), 0) AS Budget FROM Capex_Budget AS a group by a.year,a.ProjectBased ) as aa left join
(select b.year, b.ProjectBased AS Project_based,ISNULL(b.HKVendorCategory, N'N/A') AS Vendor,b.Nature AS Nature,ISNULL(SUM(b.BABudgetDetails), 0) AS BA_Budget_Details from  CAPEX_Main AS b where b.year is not null group by b.year, b.ProjectBased,b.HKVendorCategory,b.Nature) as bb on aa.Year=bb.year and aa.Project_based = bb.Project_based</CommandText>
      </Query>
      <Fields>
        <Field Name="Project_based">
          <DataField>Project_based</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Budget">
          <DataField>Budget</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
