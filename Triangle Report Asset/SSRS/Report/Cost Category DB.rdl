        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HKCategory">
          <DataField>HKCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="preYear">
          <DataField>preYear</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Budget">
          <DataField>Budget</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="curYear">
          <DataField>curYear</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="nexYear">
          <DataField>nexYear</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
