          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HKVendorCategory">
          <DataField>HKVendorCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="val">
          <DataField>val</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
