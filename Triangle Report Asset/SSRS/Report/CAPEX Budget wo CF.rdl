  <DataSets>
    <DataSet Name="HKnProject">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <CommandText>select aa.Year, aa.Project_based,aa.Budget,bb.BA_Budget_Details from
(select ds.Year, ds.Project_based,sum(ISNULL(a.BudgetDetail, 0)) AS Budget from ((select distinct a.Year,a.ProjectBased AS Project_based FROM Capex_Budget AS a where a.Year is not null and Status = 'Planned') union (select distinct b.Year,b.ProjectBased AS Project_based FROM CAPEX_Main AS b where b.Year is not null)) ds left join Capex_Budget AS a on a.ProjectBased = ds.Project_Based AND ds.Year = a.Year and Status = 'Planned'  group by ds.Year, ds.Project_based) as aa left join
(select ds.Year, ds.Project_based,ISNULL(SUM(b.BABudgetDetails), 0) AS BA_Budget_Details
from ((select distinct a.Year,a.ProjectBased AS Project_based FROM Capex_Budget AS a where a.Year is not null and Status = 'Planned') union (select distinct b.Year,b.ProjectBased AS Project_based FROM CAPEX_Main AS b where b.Year is not null)) ds left join CAPEX_Main AS b on ds.Project_Based = b.ProjectBased AND b.Year = ds.Year group by ds.Year, ds.Project_based) as bb 
on aa.Year = bb.Year and aa.Project_based = bb.Project_based</CommandText>
      </Query>
      <Fields>
        <Field Name="Project_based">
          <DataField>Project_based</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Budget">
          <DataField>Budget</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="BA_Budget_Details">
          <DataField>BA_Budget_Details</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
