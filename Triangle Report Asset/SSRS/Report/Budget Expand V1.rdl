  <DataSets>
    <DataSet Name="HKnProject">
      <Query>
        <DataSourceName>budgetDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select * from (
(select Project, year, sum(BudgetDetail) as BudgetDetail,status,ExpandValue as Expand,Security,'budget' as flag from CAPEX_Budget where status not like IIF(year = '2018','','CF from%') group by Project, year,Status,ExpandValue,Security) 
union  
(select Project, year, sum(BABudgetDetails) as BABudgetDetails,status,Expand,Security,'consumed' as flag from CAPEX_Main 
where status not like IIF(year = '2018','','CF from%')
group by Project, year,Status,Expand,Security) 
) as aaaa where flag = IIF (@Year &lt;= YEAR(getdate()) ,'consumed','budget') and Year  = @Year</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational">
            <SelectedColumns>
              <ColumnExpression ColumnOwner="CAPEX Budget" ColumnName="Year" />
              <ColumnExpression ColumnOwner="CAPEX Budget" ColumnName="Expand" />
              <ColumnExpression ColumnOwner="CAPEX Budget" ColumnName="Title" />
              <ColumnExpression ColumnOwner="CAPEX Budget" ColumnName="Security" />
